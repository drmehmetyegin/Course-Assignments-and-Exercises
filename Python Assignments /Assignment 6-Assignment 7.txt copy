    "numbers = [1, 3, 7, 4, 3, 0, 3, 6, 3]\n",
    "unique=list(set(numbers)) #unique=[0, 1, 3, 4, 6, 7]\n",
    "len(unique)   # len(unique)=6 so we have indexes from 0 to 5\n",
    "freq_numb=[numbers.count(unique[0]),numbers.count(unique[1]),\n",
    "           numbers.count(unique[2]),numbers.count(unique[3]),\n",
    "           numbers.count(unique[4]),numbers.count(unique[5])]\n",
    "z=max(freq_numb)\n",
    "y=freq_numb.index(z)\n",
    "f\"the most frequent number is {unique[y]} and it was {z} times repeated\""
